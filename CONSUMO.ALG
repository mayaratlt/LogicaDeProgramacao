Algoritmo "consumo"



Var

   distancia, combustivel, consumo: real



Inicio

    escreval("Distancia percorrida: ")
    leia(distancia)
    escreval("Combustivel gasto: ")
    leia(combustivel)
    
    
    consumoMedio <- distancia / combustivel
    
    escreva("Consumo medio = ", consumoMedio:5:3)


Fimalgoritmo